{
  "general": {
    "dependencies": [
      "Values.hs",
      "EvaluationUtils.hs"
    ],
    "selector": true
  },
  "extensions": {
    "file": "hs"
  },
  "formats": {
    "namespace": "pascal_case",
    "function": "camel_case"
  },
  "constructs": {
    "heterogeneous_collections": false,
    "heterogeneous_arguments": false,
    "objects": false
},
  "datatypes": {
    "int8": "supported",
    "uint8": "supported",
    "int16": "supported",
    "uint16": "supported",
    "int32": "supported",
    "uint32": "supported",
    "int64": "supported",
    "uint64": "supported",
    "single_precision": "supported",
    "double_extended": "unsupported",
    "fixed_precision": "unsupported",
    "array": "unsupported",
    "set": "unsupported",
    "map": "unsupported",
    "list": "supported",
    "tuple": "supported"
  }
}
